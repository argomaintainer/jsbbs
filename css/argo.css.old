#maillist-holder{
    border-top:1px solid #bbb;
    padding-top:.5em;
}

#maillist-holder>li{
    cursor:pointer;
    padding-left:1em;
    border-left:2px solid transparent;
}

#maillist-holder>li.read0{
    font-weight:bold;
}

#maillist-holder>li:nth-child(even){
    background:#f0f0f0;
}

#mail-container{
    padding:0 2em 2em 2em;
}

time{
    display:inline-block;
    color:#777;
    font-size:.8em;
}

#maillist-holder>li.selected{
    border-left:2px solid #488;
}

#editor-holder-inner{
    background:#f0f0f0;
    padding:2em;
    border-top:1px solid #bbb;
}

.cell-inner{
    margin:1em;
}

.cell{
    background:#fcfcfc;
    border:1px solid #ccc;
    border-radius: 5px;
    border-bottom:3px solid #bbb;
}

